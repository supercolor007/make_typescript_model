<idea-plugin>
  <id>com.roy.common.plugin.typescript</id>
  <name>MakeTypeScriptModel</name>
  <version>1.0</version>
  <vendor email="993914116@qq.com" url="https://github.com/supercolor007">supercolor</vendor>

  <description><![CDATA[
      Make Java model to TypeScript model. For example, Java model: Class Info { <br>
      private Integer id;<br>
      private String name;<br>
      // getter setter<br>
      }<br>
      to typeScript model: export interface Info {<br>
      id: number;<br>
      name: string;<br>
      }<br>
    ]]></description>

  <change-notes><![CDATA[
      init make model to typescript, 993914116@qq.com.<br>
      <em>init</em>
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
      <action id="make.typescript.model" class="com.roy.common.typescript.MakeTypeScriptModel" text="MakeTypeScriptModel"
              description="com.roy.common.typescript.MakeTypeScriptModel">
          <add-to-group group-id="GenerateGroup" anchor="first"/>
      </action>
  </actions>

</idea-plugin>